  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Jest Tests",
      "type": "node",
      "request": "launch",
      "program": "${workspaceRoot}/ui/node_modules/jest/bin/jest.js",
      "args": [
        "--runInBand",
        "--config=${workspaceRoot}/ui/jest.config.js",
        "--watch",
